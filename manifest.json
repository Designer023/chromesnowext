{
  "name": "Snow Mo Fo",
  "version": "0.6",
  "manifest_version": 2,
  "description": "Make it snow on every webpage",
  "background": "background.html",
  "browser_action": {
    "name": "Snow?",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery.js", "content.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
